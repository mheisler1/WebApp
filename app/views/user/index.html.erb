<h2><%= current_user.company %> Job Postings</h2>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Company</th>
            <th>Job Type</th>
            <th>Approved?</th>
        </tr>
    </thead>
    <tbody>
        <% @job_postings.each do |job_posting|%>
        <tr>
            <td><%= link_to job_posting.title, job_posting %></td>
            <td><%= job_posting.company %></td>
            <td><%= job_posting.job_type.job_type %></td>
            <td><%= job_posting.approved %></td>
        </tr>
        <% end %>
    </tbody>
</table>
<br>



<%= link_to "Display Page", display_path %>
<br><br>
<%=link_to "New Job", :controller=>"job_postings", :action=>"new" %>